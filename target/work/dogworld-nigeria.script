CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE AUDITTRAIL(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ACTION VARCHAR(255),CRUD_OPERATION VARCHAR(255),TIMEOFOPERATION TIMESTAMP,USERNAME VARCHAR(255))
CREATE MEMORY TABLE CUSTOMERINQUIRY(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CONTACTEMAIL VARCHAR(255),CONTACTNAME VARCHAR(255),CONTACTPHONE VARCHAR(255),MESSAGE VARCHAR(255),SUBJECT VARCHAR(255),TIMERECEIVED VARCHAR(255),CONSTRAINT SYS_CT_48 UNIQUE(ID))
CREATE MEMORY TABLE DOG(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,BIRTHLOCATION VARCHAR(255),BREEDID VARCHAR(255),COLOUR VARCHAR(255),DATEOFBIRTH DATE,DESCRIPTION VARCHAR(255),GENDER VARCHAR(255),HEIGHT INTEGER NOT NULL,NAME VARCHAR(255),OWNERID BIGINT,STATUS VARCHAR(255),TAGNUMBER VARCHAR(255))
CREATE MEMORY TABLE DOGOWNER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CONTACTADDRESS VARCHAR(255),DATEOFCREATION DATE,EMAILADDRESS VARCHAR(255),FIRSTNAME VARCHAR(255),KENNELNAME VARCHAR(255),LASTNAME VARCHAR(255),MOBILENUMBER VARCHAR(255),WEBSITE VARCHAR(255))
CREATE MEMORY TABLE STATE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255))
CREATE MEMORY TABLE DOGBREED(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DESCRIPTION VARCHAR(255),NAME VARCHAR(255))
CREATE MEMORY TABLE DOGREQUEST(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CONTACTEMAIL VARCHAR(255),CONTACTPHONE VARCHAR(255),PREFERREDBREED VARCHAR(255),PREFERREDCOLOUR VARCHAR(255),PREFERREDGENDER VARCHAR(255),QUANTITYNEEDED INTEGER NOT NULL,REQUESTDESCRIPTION VARCHAR(255),REQUESTLOCATION VARCHAR(255),REQUESTSTATUS VARCHAR(255),CONSTRAINT SYS_CT_60 UNIQUE(ID))
CREATE MEMORY TABLE BREEDER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ADDRESS VARCHAR(255),BREEDS VARBINARY(255),EMAIL VARCHAR(255),LOGO VARBINARY(255),NAME VARCHAR(255),TELEPHONE VARCHAR(255),CONSTRAINT SYS_CT_64 UNIQUE(NAME),CONSTRAINT SYS_CT_65 UNIQUE(TELEPHONE),CONSTRAINT SYS_CT_66 UNIQUE(EMAIL),CONSTRAINT SYS_CT_67 UNIQUE(ID))
